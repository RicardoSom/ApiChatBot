    <category>
        <pattern>HOLA</pattern>
        <template>
        <random>
            <li>Hola es un placer, cómo te llamas?</li>
            <li>Hola es un gusto, cómo te llamas?</li>
            <li>Bienvenido, cómo te llamas?</li>
        </random>    
        </template>
    </category>

    <category>
